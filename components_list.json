        "title": "Params",
        "type": "object"
      },
      "inputs": {
        "properties": {
          "file_data": {
            "anyOf": [
              { "format": "binary", "type": "string" },
              { "type": "null" }
            ],
            "title": "File Data"
          },
          "file_text": {
            "anyOf": [
              { "type": "string" },
              { "type": "null" }
            ],
            "title": "File Text"
          }
        },
        "required": ["file_data", "file_text"],
        "title": "Inputs",
        "type": "object"
      },
      "outputs": null
    },
