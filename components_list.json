            "title": "Attachment"
          },
          "body": {
            "anyOf": [
              { "type": "string" },
              { "type": "null" }
            ],
            "title": "Body"
          },
          "html_body": {
            "anyOf": [
              { "type": "string" },
              { "type": "null" }
            ],
            "title": "Html Body"
          },
          "subject": { "title": "Subject", "type": "string" }
        },
        "required": ["attachment", "body", "html_body", "subject"],
        "title": "Inputs",
        "type": "object"
      },
      "outputs": null
    },
    {
      "name": "Display",
      "description": "A component that displays content to the user interface.",
      "params": null,
      "inputs": {
        "properties": {
          "text": {
            "anyOf": [
              { "type": "string" },
              { "type": "null" }
            ],
            "title": "Text"
          },
          "image": {
            "anyOf": [
              { "format": "binary", "type": "string" },
              { "type": "null" }
            ],
