  "transformation_components": [
    {
      "name": "LLM",
      "description": "A component that interacts with a large language model to generate text based on input.",
      "params": {
        "properties": {
          "prompt": { "title": "Prompt", "type": "string" }
        },
        "required": ["prompt"],
        "title": "Params",
        "type": "object"
      },
      "inputs": {
        "properties": {
          "text": {
            "anyOf": [
              { "items": { "type": "string" }, "type": "array" },
              { "type": "null" }
            ],
            "title": "Text"
          },
          "img": {
            "anyOf": [
              { "items": { "format": "binary", "type": "string" }, "type": "array" },
              { "type": "null" }
            ],
            "title": "Img"
          }
        },
        "required": ["text", "img"],
        "title": "Inputs",
        "type": "object"
      },
      "outputs": {
        "properties": {
          "response": { "title": "Response", "type": "string" }
        },
        "required": ["response"],
        "title": "Outputs",
        "type": "object"
      }
    },
    {
      "name": "Speech to Text",
      "description": "A component that converts speech audio input into text transcription.",
      "params": {
        "properties": {
          "prompt": {
            "anyOf": [
              { "type": "string" },
              { "type": "null" }
            ],
            "title": "Prompt"
          }
        },
        "required": ["prompt"],
        "title": "Params",
        "type": "object"
      },
      "inputs": {
        "properties": {
          "audio": {
            "format": "binary",
            "title": "Audio",
            "type": "string"
          },
          "filename": {
            "default": "audio.mp3",
            "title": "Filename",
            "type": "string"
          },
          "language": {
            "anyOf": [
              { "type": "string" },
              { "type": "null" }
            ],
            "default": "en",
            "title": "Language"
          }
        },
        "required": ["audio"],
        "title": "Inputs",
        "type": "object"
      },
      "outputs": {
        "properties": {
          "transcript": { "title": "Transcript", "type": "string" }
        },
        "required": ["transcript"],
        "title": "Outputs",
        "type": "object"
      }
    }
  ],
  "output_components": [
    {
      "name": "Save to Folder",
      "description": "A component that saves input file data to a specified folder path.",
      "params": {
        "properties": {
          "file_name": { "title": "File Name", "type": "string" },
          "folder_path": { "title": "Folder Path", "type": "string" }
        },
