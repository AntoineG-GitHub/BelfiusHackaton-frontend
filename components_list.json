    {
      "name": "OCR",
      "description": "A component that processes text input data.",
      "params": null,
      "inputs": {
        "properties": {
          "text_data": {
            "format": "binary",
            "title": "Text Data",
            "type": "string"
          }
        },
        "required": ["text_data"],
        "title": "Inputs",
        "type": "object"
      },
      "outputs": {
        "properties": {
          "processed_text": {
