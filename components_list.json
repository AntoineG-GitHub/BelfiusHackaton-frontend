{
  "input_components": [
    {
      "name": "Image Input",
      "description": "A component that processes image input data.",
      "params": {
        "properties": {
          "image_format": {
            "anyOf": [
              { "type": "string" },
              { "type": "null" }
            ],
            "default": "png",
            "title": "Image Format"
          }
        },
        "title": "Params",
        "type": "object"
      },
      "inputs": {
        "properties": {
          "image_data": {
            "format": "binary",
            "title": "Image Data",
            "type": "string"
          }
        },
        "required": ["image_data"],
        "title": "Inputs",
        "type": "object"
      },
      "outputs": {
        "properties": {
          "processed_image": {
            "format": "binary",
            "title": "Processed Image",
            "type": "string"
          }
        },
        "required": ["processed_image"],
        "title": "Outputs",
        "type": "object"
      }
    },
    {
      "name": "Audio Input",
      "description": "A component that processes audio input data.",
      "params": {
        "properties": {
          "sample_rate": {
            "anyOf": [
              { "type": "integer" },
              { "type": "null" }
