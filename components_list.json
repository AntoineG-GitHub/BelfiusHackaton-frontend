          }
        },
        "title": "Params",
        "type": "object"
      },
      "inputs": {
        "properties": {
          "audio_data": {
            "format": "binary",
            "title": "Audio Data",
            "type": "string"
          }
        },
        "required": ["audio_data"],
        "title": "Inputs",
        "type": "object"
      },
      "outputs": {
        "properties": {
          "processed_audio": {
            "format": "binary",
            "title": "Processed Audio",
            "type": "string"
          }
        },
        "required": ["processed_audio"],
        "title": "Outputs",
        "type": "object"
      }
    },
    {
      "name": "Document Input",
      "description": "A component that processes document input data.",
